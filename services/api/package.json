{
  "name": "earnzy-api",
  "version": "1.0.0",
  "description": "Earnzy Admin Panel API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy:dev": "aws lambda update-function-code --function-name earnzy-api-dev --zip-file fileb://dist.zip",
    "deploy:prod": "aws lambda update-function-code --function-name earnzy-api-prod --zip-file fileb://dist.zip"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.500.0",
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/s3-request-presigner": "^3.500.0",
    "@aws-sdk/util-dynamodb": "^3.500.0",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "jsonwebtoken": "^9.1.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}
